<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <meta name="viewport" content="width=device-width,initial-scale=1.0">
    <title>{{ title or metadata.title }}</title>
    <meta name="description" content="{{ description or metadata.description }}">

    {# {% set criticalCss %}
        {% include "assets/css/critical.css" %}
    {% endset %} #}

    {% if siteTools.env == 'development' %}
        <link rel="stylesheet" href="/_includes/assets/css/critical.css">
        <link rel="stylesheet" href="/_includes/assets/css/bravery.css">
    {% else %}
        <link rel="stylesheet" href="/_includes/assets/css/bravery.css?v=1.0.2" defer="defer">
    {% endif %}
    <script src="/_includes/assets/js/min/bravery.min.js" defer="defer"></script>
